public class Main {

	public static void main(String[] args) {
		TextEditor editor = new TextEditor();
		editor.add("Hello");
		editor.add(',');
		editor.delete();
		editor.add(' ');
		editor.add("world");
		// ä¿?å­˜çŠ¶æ€?:
		String state = editor.getState();
		// ç»§ç»­ç¼–è¾‘:
		editor.add("!!!");
		editor.delete();
		editor.print();
		// æ?¢å¤?çŠ¶æ€?:
		editor.setState(state);
		editor.print();
	}
}
