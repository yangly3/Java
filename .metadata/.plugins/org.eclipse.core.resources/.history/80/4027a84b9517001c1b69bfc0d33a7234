public class Main {

	public static void main(String[] args) {
		System.out.println(tax(2000, 0.1));
		System.out.println(tax(-200, 0.1));
		System.out.println(tax(2000, -0.1));
	}

	static double tax(int salary, double rate) {
		// TODO: å¦‚æžœä¼ å…¥çš„å?‚æ•°ä¸ºè´Ÿï¼Œåˆ™æŠ›å‡ºIllegalArgumentException
		return salary * rate;
	}
}
